# Allow disabling of entire environment suite
test -n "$INHERIT_ENV" && return 0
test -n "$shlogout_loaded" && return 0

sh_load_status .shlogout

. $ZDOT_RUN_HOOKS .shlogout.d

shlogout_loaded=y
